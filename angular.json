{
  "version": 1,
  "projects": {
    "configurator": {
      "projectType": "application",
      "root": "apps/configurator",
      "sourceRoot": "apps/configurator/src",
      "prefix": "oniki",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/configurator",
            "index": "apps/configurator/src/index.html",
            "main": "apps/configurator/src/main.ts",
            "polyfills": "apps/configurator/src/polyfills.ts",
            "tsConfig": "apps/configurator/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/configurator/src/favicon.ico",
              "apps/configurator/src/assets"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/configurator/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/configurator/src/environments/environment.ts",
                  "with": "apps/configurator/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "configurator:build:production"
            },
            "development": {
              "browserTarget": "configurator:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "configurator:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/configurator/src/**/*.ts",
              "apps/configurator/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/configurator"],
          "options": {
            "jestConfig": "apps/configurator/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "configurator-e2e": {
      "root": "apps/configurator-e2e",
      "sourceRoot": "apps/configurator-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/configurator-e2e/cypress.json",
            "devServerTarget": "configurator:serve:development",
            "tsConfig": "apps/configurator-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "configurator:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/configurator-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "configurator3d": {
      "projectType": "library",
      "root": "libs/configurator3d",
      "sourceRoot": "libs/configurator3d/src",
      "prefix": "oniki",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/configurator3d"],
          "options": {
            "jestConfig": "libs/configurator3d/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/configurator3d/src/**/*.ts",
              "libs/configurator3d/src/**/*.html"
            ]
          }
        }
      }
    },
    "renderer": {
      "projectType": "library",
      "root": "libs/renderer",
      "sourceRoot": "libs/renderer/src",
      "prefix": "oniki",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": ["dist/libs/renderer"],
          "options": {
            "project": "libs/renderer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/renderer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/renderer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/renderer"],
          "options": {
            "jestConfig": "libs/renderer/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/renderer/src/**/*.ts",
              "libs/renderer/src/**/*.html"
            ]
          }
        }
      }
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "oniki",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/src/**/*.ts", "libs/ui/src/**/*.html"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/angular",
    "packageManager": "npm"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "configurator"
}
